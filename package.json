    "@tamagui/theme-base": "^1.108.4",
    "expo": "~53.0.20",
    "expo-file-system": "^18.1.11",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-router": "~5.1.4",
